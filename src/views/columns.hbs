<div class="blog-columns">
  <a href="javascript:;" data-modal="postColumn" class="blog-column--item item-row-4 item-column-4 blog-column--add">
  </a>
  {{#each list}}
  <a href="javascript:;" data-router="index" data-column-id="{{_id}}" class="blog-column--item item-{{size}}">
    <p class="blog-column--name">{{name}}</p>
    <p class="blog-column--count">文章数量：{{aid.length}}</p>
  </a>
  {{/each}}
</div>